URxvt*foreground:White
URxvt*background:Black

!-*-{ Xft settings
Xft*dpi:       96
Xft*antialias: True
Xft*hinting:   Full
!-*-}

! fonts
! run "fc-list" for a list of available fonts
URxvt*font: xft:Monospace:pixelsize=14

! The string will be interpreted as if typed into the shell as-is.
! In this example, printing will be disabled altogether.
URxvt.print-pipe: "cat > /dev/null"

! Set the style of the scrollbar: rxvt (default), plain, next, xterm
URxvt.scrollstyle: rxvt
! To disable the scrollbar
URxvt.scrollBar: false
URxvt*scrollTtyOutput: false
URxvt*scrollTtyKeypress: true
URxvt*secundaryScroll: true
URxvt*saveLines : 8000

URxvt*letterSpace: -1

!-*- Perl extensions -*-
URxvt.perl-ext-common:default,clipboard,matcher,keyboard-select,tabbed
URxvt.keysym.M-u:     perl:url-select:select_next
URxvt.url-launcher:   /usr/bin/firefox
URxvt.underlineURLs:  True
URxvt.matcher.button: 1 
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.M-c:     perl:clipboard:copy
URxvt.keysym.M-v:     perl:clipboard:paste
URxvt.keysym.M-C-v:   perl:clipboard:paste_escaped
!URxvt.copyCommand:   xsel -ib
!URxvt.pasteCommand:  xsel -ob
! Comment this if you dont want copy when text is selected
URxvt.clipboard.autocopy: true

URxvt.depth : 32

URxvt.tabbed.tabbar-fg : 5
URxvt.tabbed.tabbar-bg : 0
URxvt.tabbed.tab-fg : 14
URxvt.tabbed.tab-bg : 0
